<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <lofcategorybannerslider>
            <general>
                <enabled>1</enabled>
            </general>
            <slider>
                <auto_play_slider>1</auto_play_slider>
                <interval_slider>7200</interval_slider>
                <pause_on_hover_slider>1</pause_on_hover_slider>
                <select_animation_slider></select_animation_slider>
                <loader_slider>1</loader_slider>
